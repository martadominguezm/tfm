) {\n    const { isDark } = useAppConfig()\n\n    const switchTheme = () => {\n      isDark.value = !isDark.value\n      saveEvent({ eventType: isDark.value ? 'THEME_SWITCH_DARK' : 'THEME_SWITCH_LIGHT' })\n    }\n\n    return {\n      isDark,\n      switchTheme,\n\n      // Icons\n      icons: {\n        mdiWeatherNight,\n        mdiWeatherSunny,\n      },\n    }\n  },\n}\n</script>\n\n<style>\n</style>\n"]}]}